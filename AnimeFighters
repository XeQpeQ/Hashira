local Signals = {'MouseButton1Down', 'MouseButton1Click', 'Activated'}
local Test = game:GetService("Players")["LocalPlayer"].PlayerGui.MainGui.Hatch.Buttons.Multi.Price
local Button = game:GetService("Players")["LocalPlayer"].PlayerGui.MainGui.MultiHatch.Main.Yes
local bb = game:service('VirtualUser')
game:service('Players').LocalPlayer.Idled:connect(function()
    bb:CaptureController()
    bb:ClickButton2(Vector2.new())
    ab.Text = "Roblox intentó expulsarte, pero lo evité"
    wait(2)
    ab.Text = "Estado: Activo"
end)
local HashiraUI = loadstring(game:HttpGet('https://raw.githubusercontent.com/XeQpeQ/Hashira/main/UI.lua'))()
local Wait = HashiraUI.subs.Wait
local HashiraHub = HashiraUI:CreateWindow({
	Name = "Hashira Hub",
	Themeable = {
		Info = "Project Slayers"
	}
})
local MainTab = HashiraHub:CreateTab({
	Name = "Main"
})
local FarmSection = MainTab:CreateSection({
	Name = "Farm Section"
})

task.spawn(function()
	while task.wait() do
		pcall(function()
			if getgenv().AutoFarmCoins then
			for i,v in pairs(game:GetService("Workspace").Effects:GetDescendants()) do 
            if v.Name == "Base" then 
             v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0,0,0) 
                    end 
                end 
			end
		end)
	end
end)
task.spawn(function()
	while task.wait() do
		pcall(function()
			if getgenv().AutoMaxOpenEggs then
                if Test.Text == "MAX" then
               for i,v in pairs(Signals) do
                firesignal(Button[v])
end
                 end
			end
		end)
	end
end)


FarmSection:AddToggle({
	Name = "Auto MaxOpen Eggs(AFK)",
	Flag = "aoe",
	Callback = function(Value)
		getgenv().AutoMaxOpenEggs = Value
	end
})


FarmSection:AddToggle({
	Name = "Auto Farm Coins",
	Flag = "afc",
	Callback = function(Value)
		getgenv().AutoFarmCoins = Value
	end
})
