task.spawn(function()
    while task.wait() do
    if game:GetService("Players").LocalPlayer.Character.Services:FindFirstChild("Client") then
    game:GetService("Players").LocalPlayer.Character.Services["Client"].Disabled = true
        end
    end
end)

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local VirtualInputManager = game:GetService("VirtualInputManager")
local gui, mainFrame, originalSize, isExpanded

local function toggleGUIVisibility(visible)
    if gui then
        gui.Enabled = visible
    end
end

local function toggleGUISize(expand)
    if mainFrame and originalSize then
        if expand then
            mainFrame.Size = originalSize
        else
            mainFrame.Size = UDim2.new(0, 150, 0, 100) -- Modifica el tamaño según sea necesario
        end
        isExpanded = expand
    end
end

local bb = game:GetService('VirtualUser')

game:GetService('Players').LocalPlayer.Idled:Connect(function()
    bb:CaptureController()
    bb:ClickButton2(Vector2.new())
    ab.Text = "Roblox intentó expulsarte, pero lo evité"
    wait(2)
    ab.Text = "Estado: Activo"
end)

local HashiraUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/XeQpeQ/Hashira/main/UI2.lua"))()
local win = HashiraUI:Create({
    Name = "Hashira Hub",
    StartupSound = {
        Toggle = false,
        SoundID = "rbxassetid://6958727243",
        TimePosition = 1
    }
})
local AutoFarm = win:Tab('Auto Farm')
local AutoFarm2 = win:Tab('Auto Farm2')
local Fruit = win:Tab('Fruit')

local Teleport = win:Tab('Teleport')
local uitab = win:Tab('UI')

uitab:Button('Destroy GUI', function()
    win:Exit()
end)

AutoFarm:Toggle('Auto Soldier(Lvl 1)', function(v)
    getgenv().AutoSoldier = v
end)

AutoFarm:Toggle('Auto Clown(Lvl 10)', function(v)
    getgenv().AutoClown = v
end)

AutoFarm:Toggle('Auto Smoky(Lvl 20)', function(v)
    getgenv().AutoSmoky = v
end)

AutoFarm:Toggle('Auto Tashi(Lvl 30)', function(v)
    getgenv().AutoTashi = v
end)

AutoFarm:Toggle('Auto The Clown(Lvl 75: Need go to Pirate Island )', function(v)
    getgenv().AutoTheClown = v
end)

AutoFarm:Toggle('Auto Commander(Lvl 100: Need go to Soldier Island )', function(v)
    getgenv().AutoCommander = v
end)

AutoFarm:Toggle('Auto Captain(Lvl 120: Need go to Soldier Island )', function(v)
    getgenv().AutoCaptain = v
end)

AutoFarm:Toggle('Auto The Barbaric(Lvl 145: Need go to Soldier Island )', function(v)
    getgenv().AutoTheBarbaric = v
end)

AutoFarm:Toggle('Auto Trainer Chef(Lvl 250: Need go to Nave Of Chef Island )', function(v)
    getgenv().AutoTrainerChef = v
end)

AutoFarm:Toggle('Auto Dark Leg(Lvl 300: Need go to Nave Of Chef Island )', function(v)
    getgenv().AutoDarkLeg = v
end)

AutoFarm2:Toggle('Auto Dory(Lvl 350: Need go to Nave Of Chef Island )', function(v)
    getgenv().AutoDory = v
end)

AutoFarm2:Toggle('Auto Snow Soldier(Lvl 400: Need go to Ice Island )', function(v)
    getgenv().AutoSnowSoldier = v
end)

AutoFarm2:Toggle('Auto King Snow(Lvl 450: Need go to Ice Island )', function(v)
    getgenv().AutoKingSnow = v
end)

Teleport:Button('Teleport to Pirate Island', function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(game:GetService("Workspace").SpawnPoints.Spawn2.Position))
end)

Teleport:Button('Teleport to Soldier', function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(game:GetService("Workspace").SpawnPoints.Spawn3.Position))
end)

Teleport:Button('Teleport to Nave of Chef', function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(game:GetService("Workspace").SpawnPoints.Spawn7.Position))
end)

Teleport:Button('Teleport to Ice Island', function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(game:GetService("Workspace").SpawnPoints.Spawn4.Position))
end)

Fruit:Button('Get All Fruits', function()
for _,v in pairs(game:GetService("Workspace").AllDroppedFruit:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
task.wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) 
end
end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if getgenv().AutoSoldier then
                local args = {
    [1] = "take",
    [2] = "Kill 4 Soldiers"
}

game:GetService("ReplicatedStorage"):WaitForChild("Chest"):WaitForChild("Remotes"):WaitForChild("Functions"):WaitForChild("Quest"):InvokeServer(unpack(args))

                local soldier = game:GetService("Workspace").Monster.Mon["Soldier [Lv. 1]"]
                if soldier and soldier.HumanoidRootPart then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = soldier.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)
                    if soldier.Humanoid.Health == 0 then
                        soldier:Destroy()
                    end
                end
            end
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if getgenv().AutoClown then
                local args = {
    [1] = "take",
    [2] = "Kill 5 Clown Pirates"
}

game:GetService("ReplicatedStorage"):WaitForChild("Chest"):WaitForChild("Remotes"):WaitForChild("Functions"):WaitForChild("Quest"):InvokeServer(unpack(args))
                local Clown = game:GetService("Workspace").Monster.Mon["Clown Pirate [Lv. 10]"]
                if Clown and Clown.HumanoidRootPart then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Clown.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)
                    if Clown.Humanoid.Health == 0 then
                        Clown:Destroy()
                    end
                end
            end
        end)
    end
end)

