Fruit:Button('Get All Fruits', function()
for _,v in pairs(game:GetService("Workspace").AllDroppedFruit:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
task.wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) 
end
end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            if getgenv().AutoSoldier then
                local args = {
    [1] = "take",
    [2] = "Kill 4 Soldiers"
}

game:GetService("ReplicatedStorage"):WaitForChild("Chest"):WaitForChild("Remotes"):WaitForChild("Functions"):WaitForChild("Quest"):InvokeServer(unpack(args))

                local soldier = game:GetService("Workspace").Monster.Mon["Soldier [Lv. 1]"]
                if soldier and soldier.HumanoidRootPart then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = soldier.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)
                    if soldier.Humanoid.Health == 0 then
                        soldier:Destroy()
                    end
                end
            end
        end)
    end
end)

