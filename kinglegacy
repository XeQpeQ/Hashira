                local SkySoldier = game:GetService("Workspace").Monster.Mon["Sky Soldier [Lv. 800]"]
                if SkySoldier and SkySoldier.HumanoidRootPart then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = SkySoldier.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2)
