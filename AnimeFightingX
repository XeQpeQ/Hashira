task.spawn(function()
	while task.wait(1) do
		pcall(function()
			if getgenv().AutoChest then
local crates = game:GetService("Workspace").Scriptable.Crates
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    for _, crate in ipairs(crates:GetDescendants()) do
        if crate:IsA("BasePart") then
            humanoidRootPart.CFrame = crate.CFrame
        end
    end
    local N2 = game:GetService("VirtualInputManager")
    N2:SendKeyEvent(true, Enum.KeyCode.E, false, game)
			end
		end)
	end
end)
